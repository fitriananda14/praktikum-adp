n = 0
while not (3 <= n <= 199 and (n < 100 or n > 109)):
    n = int(input("Masukkan nilai n (3-199, bukan 100-109): "))
f1 = 1
f2 = 1
sigma = f1 + f2 
i = 2
while i < n:
    fn = f1 + f2
    sigma += fn
    f1,f2 = f2,fn
    i += 1

print(f"Nilai fibonacci ke-{n} adalah {fn}")
print(f"Total dari f1, f2, ..., fn adalah {sigma}")
